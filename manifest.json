{
  "manifest_version": 2,

  "name": "meme finder",
  "description":
    "display media files in your browser history",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": ["activeTab", "tabs", "storage", "history"]
}
